{
	"name": "px16asm",
	"displayName": "Pixie 16 Assembly",
	"publisher": "RobotMan2412",
	"icon": "./icons/icon256.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/robotman2412/vscode-px16asm.git"
	},
	"description": "Pixie 16 Assembly language support",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Programming Languages", "Snippets"
	],
	"contributes": {
		"languages": [{
			"id": "px16asm",
			"aliases": ["Pixie 16 Assembly", "px16asm"],
			"extensions": [".asm",".px16"],
			"configuration": "./px16asm-language-configuration.json",
			"icon": {
				"dark": "./icons/icon32.png",
				"light": "./icons/icon32.png"
			}
		}],
		"grammars": [{
			"language": "px16asm",
			"scopeName": "source.px16asm",
			"path": "./syntaxes/px16asm.tmLanguage.json"
		}],
		"snippets": [{
			"language": "px16asm",
			"path": "./snippets/px16asm.json"
		}]
	}
}
